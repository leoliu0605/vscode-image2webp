{
  "name": "image-to-webp",
  "displayName": "Image to WebP",
  "description": "Convert JPG and PNG images to WebP format.",
  "version": "0.1.0",
  "preview": false,
  "publisher": "Leo0605",
  "icon": "images/icon.png",
  "homepage": "https://github.com/leoliu0605/vscode-image2webp",
  "repository": {
    "type": "git",
    "url": "https://github.com/leoliu0605/vscode-image2webp"
  },
  "bugs": {
    "url": "https://github.com/leoliu0605/vscode-image2webp/issues"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "license": "MIT",
  "keywords": [
    "vscode",
    "webp",
    "image",
    "convert",
    "imagemin",
    "imagemin-webp"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "image-to-webp.convert",
        "title": "Convert to WebP"
      },
      {
        "command": "image-to-webp.convert-replace",
        "title": "Convert to WebP (Replace)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "image-to-webp.convert",
          "when": "resourceExtname =~ /\\.(png|jpe?g)$/i",
          "group": "image-to-webp@1"
        },
        {
          "command": "image-to-webp.convert-replace",
          "when": "resourceExtname =~ /\\.(png|jpe?g)$/i",
          "group": "image-to-webp@2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "sharp": "^0.34.2"
  }
}
